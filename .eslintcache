r => alert(err)) \r\n    }) ; \r\n    // Classes Enrolled \r\n    classenrolled.map(classid=>{\r\n      axios.get(\"http://localhost:5000/classes/getclasses/\" + classid.id)\r\n      .then((res)=>{\r\n        //console.log(res.data.meetings)\r\n        {res.data.meetings.length>0 &&\r\n          res.data.meetings.map(meeting => {\r\n          dispatch(addMid(meeting.mid))\r\n          axios.get(\"http://localhost:5000/meetings/getmeetings/\" + meeting.mid)\r\n         .then((res) => {\r\n           dispatch(addEvent({\r\n             title: res.data[0].name ,\r\n             start: new Date(res.data[0].startTime)  ,\r\n             end:  new Date(res.data[0].endTime) \r\n           }));\r\n         })\r\n        })}\r\n      })\r\n      .catch(err => alert(err)) \r\n    }) ; \r\n    setTimeout(() => {\r\n        setBusy(false) ;\r\n    } , 3000) ;\r\n  \r\n\r\n} , [isModalOpen]) ;\r\n\r\n    return (\r\n      <div className=\"timetable\">\r\n        <Calendar className=\"timetable_background\"\r\n          localizer={localizer}\r\n          selectable={true}\r\n          //onSelectEvent={event => this.onEventClick(event)}\r\n          onSelectSlot={(slotInfo) => onSlotChange(slotInfo)}\r\n          events={isBusy ? [] : myEventsList}\r\n          startAccessor=\"start\"\r\n          endAccessor=\"end\"\r\n        />\r\n        {isModalOpen && <Event selected={selecteddate} startdate={date} endate={enddate} />}\r\n      </div>\r\n    )\r\n}\r\nexport default Timetable;","C:\\new-Teachfinity-frontend\\src\\components\\CreateClassCard.js",[],"C:\\new-Teachfinity-frontend\\src\\components\\CreateClassForm.js",["237","238"],"import React from 'react' ;\r\nimport CloseIcon from \"@material-ui/icons/Close\" ;\r\nimport {closeModal} from \"../features/createClassSlice\" ;\r\nimport {useForm} from \"react-hook-form\" ;\r\nimport {useSelector , useDispatch} from \"react-redux\" ;\r\nimport {openclassCode, selectclassCodeIsOpen , changeClassCode , selectClassCode} from \"../fe