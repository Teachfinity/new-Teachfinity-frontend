r})=> {\r\n                console.log(user) ;\r\n                db.collection(\"users\")\r\n                .doc(auth.currentUser.uid).set({\r\n                    uid : user.uid ,\r\n                    email: user.email ,\r\n                    displayName: user.displayName,\r\n                    displayPic: user.photoURL,\r\n    \r\n                })\r\n            })\r\n            .then(()=>{\r\n                db.collection(\"users\").doc(auth.currentUser.uid).get().then(res => {\r\n                    const user = res.data() ;\r\n                    \r\n                    const adduser = {\r\n                        \"uid\" : user.uid ,\r\n                        \"name\" : user.displayName ,\r\n                        \"email\" : user.email,\r\n                        \"profilePicture\" : user.displayPic\r\n                    }\r\n                    axios.post(`http://localhost:5000/users/adduser` , adduser)\r\n                    .then(res => {\r\n                        console.log(res)\r\n                    })\r\n                  }) ;\r\n            })\r\n            .catch(err => alert(err) )\r\n\r\n        }\r\n\r\n    return (\r\n\r\n        <div className=\"login__background\">\r\n            <div className=\"login__background-cover\">\r\n                <div className=\"login-container2\">\r\n                    <Row className=\"login__rowss\">\r\n                        <div>\r\n                            <div className=\"login-container1\">\r\n\r\n\r\n                                <ValidatorForm onSubmit={() => {}} >\r\n                                    {/* { this.state.loading ? <CommonLoading /> : null } */}\r\n                                    <br></br><h1>Login</h1><br></br>\r\n                                    <div className=\"textfields form-group\" >\r\n                                        {/* { loading ? <CommonLoading /> : null } */}\r\n                                        <TextValidator\r\n                         