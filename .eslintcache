os';\r\n\r\nfunction PieChart() {\r\n  /* const {\r\n    data,\r\n    outerRadius,\r\n    innerRadius,\r\n  }; */\r\n  //const [data, setData] = useState([]);\r\nconst selectClass = useSelector(selectedClass) ;\r\nconst [isBusy , setBusy] = useState(true) ;\r\n  const[ data, setData] = useState([{ label: 'angry', value: 0 }, { label: 'disgust', value: 0 },{ label: 'scared', value: 0 },\r\n  { label: 'happy', value: 0 }, { label: 'sad', value: 0 },{ label: 'surprised', value: 0 }, { label: 'neutral', value: 0 }\r\n])\r\n  var outerRadius = 200;\r\n  var innerRadius = 0;\r\n\r\n  const margin = {\r\n    top: 50, right: 50, bottom: 50, left: 50,\r\n  };\r\n\r\n  const width = 2 * outerRadius + margin.left + margin.right;\r\n  const height = 2 * outerRadius + margin.top + margin.bottom;\r\n\r\n  const colorScale = d3     \r\n    .scaleSequential()      \r\n    .interpolator(d3.interpolateCool)      \r\n    .domain([0, data.length]);\r\n\r\n  useEffect(() => {\r\n    axios.get('http://localhost:5000/classes/getclasses/'+selectClass.id)\r\n    .then((res)=>{\r\n        res.data.environment.map((item)=>{\r\n            if(item.mood==='angry'){\r\n                data[0].value++\r\n            }\r\n            if(item.mood==='disgust'){\r\n                data[1].value++\r\n            }\r\n            if(item.mood==='scared'){\r\n                data[2].value++\r\n            }\r\n            if(item.mood==='happy'){\r\n                data[3].value++\r\n            }\r\n            if(item.mood==='sad'){\r\n                data[4].value++\r\n            }\r\n            if(item.mood==='surprised'){\r\n                data[5].value++\r\n            }\r\n            if(item.mood==='neutral'){\r\n                data[6].value++\r\n            }\r\n        })\r\n    })\r\n    .then(()=>{\r\n        setBusy(false)\r\n        drawChart();\r\n    })\r\n  }, [data]);\r\n\r\n  function drawChart() {\r\n    // Remove the old svg\r\n    d3.select('#pie-container')\r\n      .select('